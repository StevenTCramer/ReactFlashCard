@{
  Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
  <meta name="viewport" content="width=device-width" />
  <title>Index</title>
  <link href="@Url.Content(contentPath:"~/Static/Style/Site.css")" rel="stylesheet" />
</head>
<body class="Site">
  <header class="Site-header">
    <div class="Header Header--cozy" role="banner">
      header
    </div>
  </header>
  <main class="Site-content">
    <div class="Container">
      <h1>{{ page.title }}</h1>
      {{ page.content }}
    </div>
  </main>
  <footer class="Site-footer">
    <div class="Footer">
      footer
    </div>
  </footer>
</body>
</html>